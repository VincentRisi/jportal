<config>
  <common>
    <IntervalTime>5</IntervalTime>
    <MaxNumServer>6</MaxNumServer>
    <NumServer>2</NumServer>
    <StartProcThreshold>70</StartProcThreshold>
    <connectMinutes>10</connectMinutes>
    <maxRestarts>10</maxRestarts>
    <maxSendSize>0</maxSendSize>
    <restartCommand>echo</restartCommand>
    <statsCheckSeconds>15</statsCheckSeconds>
  </common>
  <specific>
    <binuserver>
      <database>smdb itdb utdb</database>
      <smdb>smdev7:ssmd00:devsm:devpassword</smdb>
      <itdb>itdev7:ssmd00:devsm:devpassword</itdb>
      <utdb>utdev7:uttm:devsm:devpassword</utdb>
      <Debug>0</Debug>
      <LogFile>binuserver.log</LogFile>
      <logLevel>debug</logLevel>
      <logDisplay>1</logDisplay>
      <rotateLog>1</rotateLog>
      <Service>23456</Service>
      <python>/usr/bin/python</python>
      <datadir>/main/bbd/data/binu</datadir>
      <pythonpath>/main/bbd/bin/pyextensions;/main/bbd/bin/pyextensions/binu</pythonpath>
      <script>/main/bbd/bin/pyextensions/binu_start.py</script>
    </binuserver>
  </specific>
</config>
