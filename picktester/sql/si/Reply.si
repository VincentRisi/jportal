// used by Rutter
PROC ForUpd
INPUT
  Id            int
  QueueId       char  (16)
  Status        byte
OUTPUT (single)
  MessageID     int
  QueueID       char (16)
  MessageLen    short
  MessageData   blob(16000)
  MessageType   byte
  DateCreated   datetime
  Status        byte
  ResponseQueue char  (16)
sqlcode
   SELECT
      r.MessageID
     ,r.QueueID
     ,r.MessageLen
     ,r.MessageData
     ,r.MessageType
     ,to_char(r.DateCreated, 'YYYYMMDDHH24MISS')
     ,r.Status
     ,m.ResponseQueue
   FROM
      Reply r, Message m
   WHERE
          r.ID = :ID
      AND r.Queueid = :Queueid
      AND r.Status = :Status
      and m.id = r.MessageID
      for update
endcode


// used by Rutter
PROC UpdStatus
INPUT
  Id            int
